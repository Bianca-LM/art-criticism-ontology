    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>#Signature_Datation</IRI>
        <Literal xml:lang="en">Signature Datation</Literal>
    </AnnotationAssertion>
