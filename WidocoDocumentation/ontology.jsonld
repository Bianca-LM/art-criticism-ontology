[ {
  "@id" : "http://www.semanticweb.org/bianc/ontologies/2023/1/mat",
  "@type" : [ "http://www.w3.org/2002/07/owl#Ontology" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "OM&TA (Ontology of Materials & Techniques in Art) is an ontology which aims at representing the relationship between materials and techniques for the description of artworks in a semantic way."
  } ]
}, {
  "@id" : "http://www.semanticweb.org/bianc/ontologies/2023/1/mat#Artwork",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "The proper work of art, with all the additional elements."
  } ]
}, {
  "@id" : "http://www.semanticweb.org/bianc/ontologies/2023/1/mat#Artwork_Component",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "A subcomponent of an artwork which can have its own material and technique."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "http://www.semanticweb.org/bianc/ontologies/2023/1/mat#Artwork"
  } ]
}, {
  "@id" : "http://www.semanticweb.org/bianc/ontologies/2023/1/mat#Current_Support",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "The current support, which can be different from the original support for different reasons (restoration, demages, transfer etc.)."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "http://www.semanticweb.org/bianc/ontologies/2023/1/mat#Support"
  } ]
}, {
  "@id" : "http://www.semanticweb.org/bianc/ontologies/2023/1/mat#Decoration",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "An additional element which is main purpose is not practical or aesthetic, but brings \"decorum\" to another element."
  } ],
  "http://www.w3.org/2002/07/owl#disjointWith" : [ {
    "@id" : "http://www.semanticweb.org/bianc/ontologies/2023/1/mat#Ornament"
  } ]
}, {
  "@id" : "http://www.semanticweb.org/bianc/ontologies/2023/1/mat#Material",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "It is the proper material which the support is made of."
  } ]
}, {
  "@id" : "http://www.semanticweb.org/bianc/ontologies/2023/1/mat#Original_Support",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "The original support used by the artist."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "http://www.semanticweb.org/bianc/ontologies/2023/1/mat#Support"
  } ]
}, {
  "@id" : "http://www.semanticweb.org/bianc/ontologies/2023/1/mat#Ornament",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "An additional element other from the main body of the artwork, which helps through repeated patterns, and natural elements at the visual organization of the artwork, in which prevails the aesthetic function."
  } ]
}, {
  "@id" : "http://www.semanticweb.org/bianc/ontologies/2023/1/mat#Signature",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "The signature of the author left on the artwork itself."
  } ]
}, {
  "@id" : "http://www.semanticweb.org/bianc/ontologies/2023/1/mat#Signature_Datation",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "The datation of the signature left by the author on the artwork itself."
  } ]
}, {
  "@id" : "http://www.semanticweb.org/bianc/ontologies/2023/1/mat#Support",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "The support on which the technique is performed, \"given from nature or elaborated by human beings\" (Sciolla)."
  } ]
}, {
  "@id" : "http://www.semanticweb.org/bianc/ontologies/2023/1/mat#Technique",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "\"The union of all the principles, rules and manners elaborated by the artists for the material processing over time, experimented in the workshops and artistic laboratories\" as defined by Sciolla."
  } ]
}, {
  "@id" : "http://www.semanticweb.org/bianc/ontologies/2023/1/mat#hasTechnique",
  "@type" : [ "http://www.w3.org/2002/07/owl#ObjectProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#domain" : [ {
    "@id" : "http://www.semanticweb.org/bianc/ontologies/2023/1/mat#Artwork"
  }, {
    "@id" : "http://www.semanticweb.org/bianc/ontologies/2023/1/mat#Artwork_Component"
  }, {
    "@id" : "http://www.semanticweb.org/bianc/ontologies/2023/1/mat#Decoration"
  }, {
    "@id" : "http://www.semanticweb.org/bianc/ontologies/2023/1/mat#Ornament"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#range" : [ {
    "@id" : "http://www.semanticweb.org/bianc/ontologies/2023/1/mat#Technique"
  } ]
}, {
  "@id" : "http://www.semanticweb.org/bianc/ontologies/2023/1/mat#isMadeOf",
  "@type" : [ "http://www.w3.org/2002/07/owl#ObjectProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#domain" : [ {
    "@id" : "http://www.semanticweb.org/bianc/ontologies/2023/1/mat#Current_Support"
  }, {
    "@id" : "http://www.semanticweb.org/bianc/ontologies/2023/1/mat#Original_Support"
  }, {
    "@id" : "http://www.semanticweb.org/bianc/ontologies/2023/1/mat#Support"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#range" : [ {
    "@id" : "http://www.semanticweb.org/bianc/ontologies/2023/1/mat#Material"
  } ]
}, {
  "@id" : "http://www.semanticweb.org/bianc/ontologies/2023/1/mat#isPartOf",
  "@type" : [ "http://www.w3.org/2002/07/owl#ObjectProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#domain" : [ {
