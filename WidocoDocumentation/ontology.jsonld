  }, {
    "@id" : "http://www.semanticweb.org/bianc/ontologies/2023/1/mat#Ornament"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#range" : [ {
    "@id" : "http://www.semanticweb.org/bianc/ontologies/2023/1/mat#Artwork"
  }, {
    "@id" : "http://www.semanticweb.org/bianc/ontologies/2023/1/mat#Artwork_Component"
  } ]
}, {
  "@id" : "http://www.semanticweb.org/bianc/ontologies/2023/1/mat#isPerformedOn",
  "@type" : [ "http://www.w3.org/2002/07/owl#ObjectProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#domain" : [ {
    "@id" : "http://www.semanticweb.org/bianc/ontologies/2023/1/mat#Technique"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#range" : [ {
